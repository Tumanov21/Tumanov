@model IEnumerable<Site.Models.User>
@{
    ViewData["Title"] = "UserList";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Email</td>
            <td>
                <a asp-action="Edit" asp-controller="Roles" asp-route-userid="@item.Id" class="btn btn-outline-primary">Aсcess rights</a>
            </td>
        </tr>
    }
</table>
<a asp-action="Index" class="btn btn-outline-dark">Back</a>
