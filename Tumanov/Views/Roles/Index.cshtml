@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>
@{ 
    ViewData["Title"] = "Roles";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <tr><th>Email</th><th>Year</th></tr>
    @foreach (var item in Model)
	{
		 <tr>
             <td>@item.Name</td>
             <td>
                 <form asp-action="Delete" asp-controller="Roles" asp-route-id="@item.Id" method="post">
                     <button type="submit" class="btn btn-outline-danger">Delete</button>
                 </form>
             </td>
         </tr>
	}
</table>

<a asp-action="Create" asp-controller="Roles" class="btn btn-outline-primary">CreateRole</a>
<a asp-action="UserList" asp-controller="Roles" class="btn btn-outline-primary">UserList</a>